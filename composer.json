{
  "name": "torann/hashids",
  "description": "Laravel package for Hashids",
  "keywords": [
    "laravel",
    "hash",
    "hashid",
    "encode",
    "decode"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
    "hashids/hashids": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.3",
    "phpstan/phpstan": "^0.12.14",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Torann\\Hashids\\": "src/Torann/Hashids"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Torann\\Hashids\\HashidsServiceProvider"
      ],
      "aliases": {
        "Hashids": "Torann\\Hashids\\Facade\\Hashids"
      }
    }
  }
}
